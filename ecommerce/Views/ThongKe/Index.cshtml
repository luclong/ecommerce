
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Layout/AdminLayout.cshtml";
}
<style>
    .list_of_members{
        display: none;
    }
</style>

<div class="list_of_members1 col-md-12">
    <div class="sales">
        <div class="icon" style="margin-left:120px">
            <i class="dollar"></i>
        </div>
        <div class="icon-text">
            
            <h3>@ViewBag.TongDoanhThu.ToString("#,##") Đ</h3>
            <br />
            <h4 style="margin-left:110px; color:#fff;">Doanh Thu</h4>
        </div>
        <div class="clearfix"></div>
    </div>
    <div class="new-users">
        <div class="icon">
            <i class="user1"></i>
        </div>
        <div class="icon-text">
            <h3>@ViewBag.ThongKeThanhVien</h3>
            <p>Thành Viên</p>
        </div>
        <div class="clearfix"></div>
    </div>
    <div class="orders">
        <div class="icon">
            <i class="order"></i>
        </div>
        <div class="icon-text">
            <h3>@ViewBag.ThongKeDonHang</h3>
            <p>Đơn Hàng</p>
        </div>
        <div class="clearfix"></div>
    </div>
    <div class="visitors">
        <div class="icon">
            <i class="visit"></i>
        </div>
        <div class="icon-text">
            <h3>@ViewBag.SoNguoiTruyCap</h3>
            <p>Truy Cập</p>
        </div>
        <div class="clearfix"></div>
    </div>
    <div class="online">
        <div class="icon">
            <i class="visit"></i>
        </div>
        <div class="icon-text">
            <h3 style="color: #fff; text-align: center; font-size: 2.6em; font-weight: 500; letter-spacing: 1px;">@ViewBag.SoNguoiDangOnline</h3>
            <p style="float: left; color: #fff; font-size: 1.3em; font-weight: 300;">Online</p>
        </div>
        <div class="clearfix"></div>
    </div>
    <div class="clearfix"></div>
</div>